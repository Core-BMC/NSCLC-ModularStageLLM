  "categories": {
    "T_category": {
      "description": "Describes the size and extent of the main tumor",
      "subcategories": {
        "Tis": {
          "definition": "Carcinoma in situ",
          "criteria": []
        },
        "T1": {
          "definition": "Tumor ≤ 3 cm",
          "criteria": [
            "Surrounded by lung or visceral pleura",
            "No invasion proximal to lobar bronchus"
          ],
          "subcategories": {
            "T1mi": {
              "definition": "Minimally invasive adenocarcinoma",
              "criteria": ["≤ 3 cm with ≤ 5 mm invasion in any focus"]
            },
            "T1a": {
              "tumor_size": {"max": 1, "unit": "cm"}
            },
            "T1b": {
              "tumor_size": {"min": 1, "max": 2, "unit": "cm"}
            },
            "T1c": {
              "tumor_size": {"min": 2, "max": 3, "unit": "cm"}
            }
          }
        },
        "T2": {
          "definition": "Tumor > 3 cm to ≤ 5 cm or meeting specific criteria",
          "criteria": [
            "Involves main bronchus regardless of distance to carina",
            "Atelectasis or obstructive pneumonitis extending to hilum",
            "Visceral pleura invasion"
          ],
          "subcategories": {
            "T2a": {
              "tumor_size": {"min": 3, "max": 4, "unit": "cm"}
            },
            "T2b": {
              "tumor_size": {"min": 4, "max": 5, "unit": "cm"}
            }
          }
        },
        "T3": {
          "definition": "Tumor > 5 cm to ≤ 7 cm or invasion of specific structures",
          "criteria": [
            "Invasion of: chest wall, phrenic nerve, parietal pericardium",
            "Separate tumor nodule(s) in the same lobe as the primary tumor"
          ],
          "tumor_size": {"min": 5, "max": 7, "unit": "cm"}
        },
        "T4": {
          "definition": "Tumor > 7 cm or invasion of specific structures",
          "criteria": [
            "Invasion of: diaphragm, mediastinum, heart, great vessels, trachea, recurrent laryngeal nerve, esophagus, vertebral body, carina",
            "Separate tumor nodule(s) in a different ipsilateral lobe"
          ],
          "tumor_size": {"min": 7, "unit": "cm"}
        }
      }
    },
    "N_category": {
      "description": "Describes spread to nearby lymph nodes",
      "subcategories": {
        "N0": {
          "definition": "No regional lymph node metastasis",
          "criteria": []
        },
        "N1": {
          "definition": "Metastasis in ipsilateral peribronchial and/or hilar nodes",
          "criteria": []
        },
        "N2": {
          "definition": "Metastasis in ipsilateral mediastinal and/or subcarinal nodes",
          "subcategories": {
            "N2a": {
              "definition": "Metastasis in a single ipsilateral mediastinal or in the subcarinal nodal station",
              "criteria": [
                "Single station involvement in mediastinal nodes",
                "OR single station involvement in subcarinal node"
              ]
            },
            "N2b": {
              "definition": "Metastases in multiple ipsilateral mediastinal nodal stations with or without involvement of the subcarinal nodal station",
              "criteria": [
                "Multiple station involvement in mediastinal nodes",
                "Includes cases with or without subcarinal node involvement"
              ]
            }
          },
          "special_considerations": []
        },
        "N3": {
          "definition": "Metastasis in contralateral mediastinal, contralateral hilar, ipsilateral or contralateral scalene, or supraclavicular nodes",
          "criteria": []
        }
      }
    },
    "M_category": {
      "description": "Describes metastasis to other parts of the body",
      "subcategories": {
        "M0": {
          "definition": "No distant metastasis",
          "criteria": []
        },
        "M1": {
          "definition": "Distant metastasis",
          "subcategories": {
            "M1a": {
              "definition": "Intrathoracic metastasis",
              "criteria": [
                "Separate tumor nodule(s) in contralateral lobe",
                "Pleural or pericardial nodules",
                "Malignant pleural or pericardial effusion"
              ]
            },
            "M1b": {
              "definition": "Single extrathoracic metastasis",
              "criteria": [
                "Single metastatic lesion in one extrathoracic organ",
                "Including non-regional lymph nodes"
              ]
            },
            "M1c": {
              "definition": "Multiple extrathoracic metastases",
              "subcategories": {
                "M1c1": {
                  "definition": "Multiple extrathoracic metastases in a single organ system",
                  "criteria": [
                    "Multiple metastatic lesions confined to one organ system",
                    "Example: Multiple bone metastases count as M1c1",
                    "Example: Multiple brain metastases count as M1c1"
                  ],
                  "special_notes": []
                },
                "M1c2": {
                  "definition": "Multiple extrathoracic metastases in multiple organ systems",
                  "criteria": [
                    "Metastatic lesions in two or more different organ systems",
                    "Example: Bone + liver metastases = M1c2",
                    "Example: Brain + adrenal metastases = M1c2"
                  ]
                }
              }
            }
          }
        }
      }
    }
  },
  "stage_grouping": {
    "description": "Combines T, N, and M categories to determine overall stage",
    "stages": {
      "0": {
        "criteria": [{"T": "Tis", "N": "N0", "M": "M0"}]
      },
      "IA1": {
        "criteria": [
          {"T": "T1mi", "N": "N0", "M": "M0"},
          {"T": "T1a", "N": "N0", "M": "M0"}
        ]
      },
      "IA2": {
        "criteria": [{"T": "T1b", "N": "N0", "M": "M0"}]
      },
      "IA3": {
        "criteria": [{"T": "T1c", "N": "N0", "M": "M0"}]
      },
      "IB": {
        "criteria": [{"T": "T2a", "N": "N0", "M": "M0"}]
      },
      "IIA": {
        "criteria": [
          {"T": "T2b", "N": "N0", "M": "M0"},
          {"T": ["T1a", "T1b", "T1c"], "N": "N1", "M": "M0"}
        ]
      },
      "IIB": {
        "criteria": [
          {"T": ["T1a", "T1b", "T1c"], "N": "N2a", "M": "M0"},
          {"T": "T2a", "N": "N1", "M": "M0"},
          {"T": "T2b", "N": "N1", "M": "M0"},
          {"T": "T3", "N": "N0", "M": "M0"}
        ]
      },
      "IIIA": {
        "criteria": [
          {"T": ["T1a", "T1b", "T1c"], "N": "N2b", "M": "M0"},
          {"T": ["T2a"], "N": "N2a", "M": "M0"},
          {"T": ["T2b"], "N": "N2a", "M": "M0"},
          {"T": "T3", "N": ["N1", "N2a"], "M": "M0"},
          {"T": "T4", "N": ["N0", "N1"], "M": "M0"}
        ]
      },
      "IIIB": {
        "criteria": [
          {"T": ["T1a", "T1b", "T1c"], "N": "N3", "M": "M0"},
          {"T": ["T2a", "T2b"], "N": ["N2b", "N3"], "M": "M0"},
          {"T": "T3", "N": ["N2b"], "M": "M0"},
          {"T": "T4", "N": ["N2a", "N2b"], "M": "M0"}
        ]
      },
      "IIIC": {
        "criteria": [
          {"T": ["T3", "T4"], "N": "N3", "M": "M0"}
        ]
      },
      "IVA": {
        "criteria": [{"T": "Any T", "N": "Any N", "M": ["M1a", "M1b"]}]
      },
      "IVB": {
        "criteria": [{"T": "Any T", "N": "Any N", "M": ["M1c1", "M1c2"]}]
      }
    }
  },
